(function(f,b){if(!b.__SV){var e,g,i,h;window.mixpanel=b;b._i=[];b.init=function(e,f,c){function g(a,d){var b=d.split(".");2==b.length&&(a=a[b[0]],d=b[1]);a[d]=function(){a.push([d].concat(Array.prototype.slice.call(arguments,0)))}}var a=b;"undefined"!==typeof c?a=b[c]=[]:c="mixpanel";a.people=a.people||[];a.toString=function(a){var d="mixpanel";"mixpanel"!==c&&(d+="."+c);a||(d+=" (stub)");return d};a.people.toString=function(){return a.toString(1)+".people (stub)"};i="disable time_event track track_pageview track_links track_forms track_with_groups add_group set_group remove_group register register_once alias unregister identify name_tag set_config reset opt_in_tracking opt_out_tracking has_opted_in_tracking has_opted_out_tracking clear_opt_in_out_tracking start_batch_senders people.set people.set_once people.unset people.increment people.append people.union people.track_charge people.clear_charges people.delete_user people.remove".split(" ");
    for(h=0;h<i.length;h++)g(a,i[h]);var j="set set_once union unset remove delete".split(" ");a.get_group=function(){function b(c){d[c]=function(){call2_args=arguments;call2=[c].concat(Array.prototype.slice.call(call2_args,0));a.push([e,call2])}}for(var d={},e=["get_group"].concat(Array.prototype.slice.call(arguments,0)),c=0;c<j.length;c++)b(j[c]);return d};b._i.push([e,f,c])};b.__SV=1.2;e=f.createElement("script");e.type="text/javascript";e.async=!0;e.src="undefined"!==typeof MIXPANEL_CUSTOM_LIB_URL?
    MIXPANEL_CUSTOM_LIB_URL:"file:"===f.location.protocol&&"//cdn.mxpnl.com/libs/mixpanel-2-latest.min.js".match(/^\/\//)?"https://cdn.mxpnl.com/libs/mixpanel-2-latest.min.js":"//cdn.mxpnl.com/libs/mixpanel-2-latest.min.js";g=f.getElementsByTagName("script")[0];g.parentNode.insertBefore(e,g)}})(document,window.mixpanel||[]);

const MIXPANEL = {
    init: function () {
        window.onload = function(){
            const path = location.pathname.replace(/\/$/, '');
            const params = new URLSearchParams(location.search);
	        const maxRetries = 5; // 최대 재시도 횟수
	        let attempt = 0; // 현재 재시도 횟수 카운터
	        const delay = 1000; // 재시도 사이의 지연 시간 (예: 1000ms = 1초)
	        // 채널톡의 shadowDOM에 일반적인 접근이 되지않습니다.
	        // 즉 믹스패널.js가 채널톡 이벤트를 잡으려면 shadowDOM에 접근하여 이벤트를 잡아야하는데, 채널톡의 shadowDOM은 자체적으로 로드하다보니(믹스패널.js와 로드순서가 미스매치됩니다.) 계속 shadowDOM안에 요소들의 이벤트를 잡지 못하는 경우가 생겼습니다. 따라서 믹스패널.js에서 잡아줄수있도록 추가된 함수입니다.
	        // 채널톡 플러그인 체크 함수
	        function channelTalkPluginCheck() {
		        attempt++; // 시도 횟수 증가

		        let hostElement = document.querySelector('#ch-plugin-entry > div');

		        if (hostElement && hostElement.shadowRoot) {
			        let shadowRoot = hostElement.shadowRoot;
			        let targetElement = shadowRoot.querySelector("#ch-shadow-root-wrapper > div > button > div");

			        if (targetElement) {
				        // 요소를 찾았으므로, 여기에 이벤트 리스너 등록 등의 로직을 추가합니다.
				        targetElement.addEventListener('click', function(e) {
					        let channelTalkStatus = document.querySelector("#ch-plugin-script").className;
					        // 초기에는 해당 클래스만 있어서 이렇게 잡았습니다.
					        if (channelTalkStatus === 'ch-messenger-hidden') {
						        mixpanel.track('click_channeltalk_open', {'site_code':SITE_CODE,'where_at': 'bo'});
					        }
					        // ch-messenger-hidden 라는 클래스로 채널톡을 끄고 키는 방식이라, 킬 때만 이벤트가 추적되게 처리하였습니다.
					        if (channelTalkStatus === 'ch-desk-messenger') {
						        mixpanel.track('click_channeltalk_close', {'site_code':SITE_CODE,'where_at': 'bo'});
					        }
					        if (channelTalkStatus === 'ch-desk-messenger ch-messenger-hidden') {
						        mixpanel.track('click_channeltalk_open', {'site_code':SITE_CODE,'where_at': 'bo'});
					        }
				        });

				        // 성공적으로 요소를 찾았으므로, 재시도를 중단합니다.
				        return; // 함수를 종료하여 더 이상의 코드 실행을 방지합니다.
			        }
		        }

		        // 요소를 찾지 못했거나 shadowRoot가 없는 경우
		        if (attempt <= maxRetries) {
			        // 최대 재시도 횟수에 도달하지 않았으므로 재시도합니다.
			        setTimeout(channelTalkPluginCheck, delay); // 일정 시간 후에 함수를 다시 호출합니다.
		        } else {
			        // 최대 재시도 횟수에 도달했습니다. 더 이상 재시도하지 않고 넘깁니다.
		        }
	        }

	        // 첫 시도를 시작합니다.
	        channelTalkPluginCheck();

	        $("#main-menu").on("click", "> li", function(e) {

		        let menuName = $(this).find("> a").first().text().replace(/\d+$/, '');
		        mixpanel.track('click_bo_gnb_menu_depth_0', {
					  'site_code':SITE_CODE,
		              'menu_name': menuName
		            });
		        // 이벤트 버블링을 방지합니다.
		        e.stopPropagation();
	        });

			// 하위 메뉴 클릭
	        $("#main-menu li ul li").click(function(e) {

		        // 클릭된 하위 메뉴의 상위 메뉴이름을 가져옵니다.
		        let parentDepth0Text = $(this).closest("ul").closest("li").find("> a").text();
		        mixpanel.track('click_bo_gnb_menu_depth_1', {
			        'site_code':SITE_CODE,
			        'menu_name': $(e.currentTarget).text().replace(/\d+$/, ''),
			        'depth_0_menu_name': parentDepth0Text.replace(/\d+$/, '')
		        });

		        // 이벤트 버블링을 방지합니다.
		        e.stopPropagation();
	        });

            // CRM 캠페인 자동화 클릭
            $("#main-menu [data-title='crm_campaigns']").click(function(e) {
                mixpanel.track('view_bo_crm_campaign', {
                    'site_code':SITE_CODE,
                });
            });

            // 좌측하단 언어 선택
            $('.menubar-foot-panel').on('click', '.dropdown-menu a', function (e) {
                mixpanel.track('click_bo_change_language', {
	                'site_code':SITE_CODE,
                    'language': $(e.currentTarget).data('language')
                });
            });

            // 사이트 관리 클릭
            $('#main-menu').on('click', '.top-menu a', function (e) {
                mixpanel.track('click_bo_gnb_sitemanage',{'site_code':SITE_CODE});
            });


            // 모바일 햄버거 클릭
            $('.top_header').on('click', '.menubar-toggle', function (e) {
                mixpanel.track('click_bo_hamburger_menu',{'site_code':SITE_CODE});
            });

            $('.headerbar-right').on('click', '.__upgrade-btn', function (e) {
                // 연장/업그레이드 버튼 클릭
                mixpanel.track('click_bo_dashboard_upgrade',{'site_code':SITE_CODE});
            }).on('click', '.__exit-btn', function (e) {
                // 나가기 버튼 클릭
                mixpanel.track('click_bo_dashboard_exit',{'site_code':SITE_CODE});
            });

						// 알림 버튼 클릭
	          $('._gnb_notification_button').on('click', function() {
							mixpanel.track('click_bo_notification', { 'site_code': SITE_CODE });
	          });

            switch (path) {
                // 대시보드
                case '/admin':
                    mixpanel.track('view_bo_dashboard', {
	                    'site_code':SITE_CODE,
                        'source': document.referrer,
                        'language': ADMIN_LANG_CODE
                    });
                    // 내사이트 새탭 열기 클릭
                    $('.brand-holder').on('click',function(e){
                      mixpanel.track('click_bo_dashboard_title',{'site_code':SITE_CODE})
                    })
                    // 프로필 영역(우측) 클릭
                    $('#mixpanel_dashboard_profile').on('click',function(e){
                      mixpanel.track('click_bo_dashboard_profile',{'site_code':SITE_CODE})
                    })
                    // 사용버전 업그레이드 클릭 (우측)
                    $('#mixpanel_dashboard_upgrade_right').on('click',function(e){
                      mixpanel.track('click_bo_dashboard_upgrade_right',{'site_code':SITE_CODE})
                    })
                    // PG 설정 Hover 버튼 클릭 (우측)
                    $('#mixpanel_dashboard_pg').on('click',function(e){
                      mixpanel.track('click_bo_dashboard_pg',{
                        'site_code': SITE_CODE,
                        'type': 'hover',
                      })
                    });
                    // PG 설정버튼 클릭 (우측)
                    $('#mixpanel_dashboard_pg_btn').on('click',function(e){
                      mixpanel.track('click_bo_dashboard_pg',{
                        'site_code': SITE_CODE,
                        'type': 'button',
                      })
                    });
	                // 우측 슬라이드 배너 클릭
	                $('div[id^="mixpanel_banner_"]').on('click', function(e) {
		                // 'data-index' 값 가져오기
		                const index = $(this).data('index');
		                const id = $(this).attr('id');
		                // 'id'에서 'mixpanel_banner_' 이후의 이름 가져오기
		                const value = id.substring('mixpanel_banner_'.length);

		                // 'index', 'id' 또는 'value'가 없으면 기록안함
		                if (typeof index !== 'number' || !id || !value) {
			                return;
		                }

		                // Mixpanel 이벤트 추적
		                mixpanel.track('click_bo_dashboard_banner_right', {'index': index, 'name': value, 'site_code': SITE_CODE});
	                });



	                // 네이버 페이 배너 클릭 (우측)
                    $('#mixpanel_dashboard_npay_banner_right').on('click',function(e){

                      mixpanel.track('click_bo_dashboard_npay_banner_right',{'site_code':SITE_CODE});


                    })
                    // 사이트 이용 기간이 만료되었습니다 팝업 보기
                    if($('#mixpanel_dashboard_popup_expired').length){
                      mixpanel.track('view_bo_dashboard_popup_expired',{'site_code':SITE_CODE});

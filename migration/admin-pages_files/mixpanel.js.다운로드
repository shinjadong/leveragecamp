                    $('#go_additional_service').on('click', function () {
                        // 통계 배너 자세히 알아보기 클릭
                        mixpanel.track('click_bo_dashboard_stat_banner',{'site_code':SITE_CODE});
                    })

                    // 상품 구매평 클릭
                    $('.shopping-review').on('click', 'a', function (e) {
                        const $target = $(e.currentTarget);
                        let type = '';

                        if ($target.hasClass('answers-title')) {
                            type = 'title';
                        } else if ($target.hasClass('answers-item')) {
                            type = 'item';
                        }

                        if (type) {
                            mixpanel.track('click_bo_dashboard_review', {
	                            'site_code':SITE_CODE,
                                'type': type
                            });
                        }
                    });

                     // 상품 문의 클릭
                     $('.shopping-qna').on('click', 'a', function (e) {
                        const $target = $(e.currentTarget);
                        let type = '';


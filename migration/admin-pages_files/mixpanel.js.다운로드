                        });
                    }

                    // 옵션 추가 버튼 클릭
                    $('._option_list').on('click', '.btn-option', function (e) {
                        mixpanel.track('click_bo_booking_option_create',{'site_code':SITE_CODE});
                    });
                    break;
                // 예약 > 성수기/휴일 관리
                case '/admin/booking/season':
                    // 성수기/휴일 관리 보기
                    mixpanel.track('view_bo_booking_season',{'site_code':SITE_CODE});
                    break;
                // 예약 > 예약 설정
                case '/admin/booking/config':
                    // 예약 설정 보기
                    mixpanel.track('view_bo_booking_settings',{'site_code':SITE_CODE});
                    break;
                // 컨텐츠 관리 > 게시물 관리
                case '/admin/contents/post':
                    if (!params.get('mode')) {
                        // 게시물 관리 보기
                        mixpanel.track('view_bo_content_post',{'site_code':SITE_CODE});
                    }

                    // 게시물 클릭
                    $('.section-body').on('click', 'a.post-link', function (e) {
                        mixpanel.track('click_bo_content_post_detail',{'site_code':SITE_CODE});
                    });

                    // 게시판 클릭
                    $('.section-body').on('click', '[id^="board_item_"]', function (e) {
                        mixpanel.track('click_bo_content_post_group',{'site_code':SITE_CODE});
                    });
                    break;
                // 컨텐츠 관리 > 댓글 관리
                case '/admin/contents/comment':
                    // 댓글 관리 보기
                    mixpanel.track('view_bo_content_comment',{'site_code':SITE_CODE});

                    // 게시물 클릭
                    $('#comment_manage_list').on('click', '.post-link', function (e) {
                        mixpanel.track('click_bo_content_comment_post',{'site_code':SITE_CODE});
                    });
                    break;
                // 컨텐츠 관리 > 입력폼 관리
                case '/admin/contents/form':
                    if (params.get('idx')) {
                        // 입력폼 상세 보기
                        mixpanel.track('view_bo_content_form_detail',{'site_code':SITE_CODE});
                    } else {
                        // 입력폼 관리 보기
                        mixpanel.track('view_bo_content_form',{'site_code':SITE_CODE});
                    }

                    // 입력폼 그룹 클릭
                    $('.section-body').on('click', '[id^="board_item_"]', function (e) {
                        mixpanel.track('click_bo_content_form_group',{'site_code':SITE_CODE});
                    });
                    break;
                // 컨텐츠 관리 > 지도 관리
                case '/admin/contents/map':
                    if (params.get('code')) {
                        // 지도 상세 보기
                        mixpanel.track('view_bo_content_map_detail',{'site_code':SITE_CODE});
                    } else {
                        // 지도 관리 보기
                        mixpanel.track('view_bo_content_map',{'site_code':SITE_CODE});
                    }

                    // 지도 양식 클릭
                    $('.section-body').on('click', '[id^="board_item_"]', function (e) {
                        mixpanel.track('click_bo_content_map_group',{'site_code':SITE_CODE});
                    });
                    break;
                // 컨텐츠 관리 > 일정 관리
                case 'admin/contents/calendar':
                    if (params.get('code')) {
                        // 일정 상세 보기
                        mixpanel.track('view_bo_content_calendar_detail',{'site_code':SITE_CODE});
                    } else {
                        // 일정 관리 보기
                        mixpanel.track('view_bo_content_calendar',{'site_code':SITE_CODE});
                    }

                    // 일정 양식 클릭
                    $('.section-body').on('click', '[id^="board_item_"]', function (e) {
                        mixpanel.track('click_bo_content_calendar_group',{'site_code':SITE_CODE});
                    });
                    break;
                // 컨텐츠 관리 > 갤러리 관리
                case '/admin/contents/gallery':
                    if (params.get('code')) {
                        // 갤러리 상세 보기
                        mixpanel.track('view_bo_content_gallery_detail',{'site_code':SITE_CODE});
                    } else {
                        // 갤러리 관리 보기
                        mixpanel.track('view_bo_content_gallery',{'site_code':SITE_CODE});
                    }

                    // 갤러리 양식 클릭
                    $('.section-body').on('click', '[id^="board_item_"]', function (e) {
                        mixpanel.track('click_bo_content_gallery_group',{'site_code':SITE_CODE});
                    });
                    break;
                // 컨텐츠 관리 > 신고/차단
                case '/admin/contents/report':
                    // 신고/차단 관리 보기
                    mixpanel.track('view_bo_content_report',{'site_code':SITE_CODE});

                    // 신고/차단 관리 상태별 클릭
                    $('.section-body').on('click', '.nav li a', function (e) {
                        let $target = $(e.currentTarget).clone();
                        $target.find('span').remove();

                        mixpanel.track('click_bo_content_report_status', {
	                        'site_code':SITE_CODE,
                            'name': $target.text()
                        });
                    });
                    break;
                // 마케팅 관리 > 전환추적/리타게팅 관리
                case '/admin/marketing/trace':
                    // 전환추적/리타게팅 관리 보기
                    mixpanel.track('view_bo_marketing_tracking',{'site_code':SITE_CODE});
                    break;
                // 마케팅 관리 > 페이스북 마케팅(FBE)
                case '/admin/marketing/facebook_fbe':
                    // 페이스북 마케팅(FBE) 보기
                    mixpanel.track('view_bo_marketing_fbe',{'site_code':SITE_CODE});

                    // 시작하기 버튼 클릭
                    $('.section-body').on('click', '._open_fbe_btn', function (e) {
                        mixpanel.track('click_bo_marketing_fbe_start',{'site_code':SITE_CODE});
                    });
                    break;
                // 마케팅 관리 > 에이스카운터
		        // 파트너를 제외한 에이스카운터는 통계로 이동하였습니다. 현재는 파트너만 기록하고 있는 값입니다.
                case '/admin/marketing/ace_counter':
                    // 에이스카운터 보기
                    mixpanel.track('view_bo_marketing_ace',{'site_code':SITE_CODE});

                    // 신청하기 버튼 클릭
                    $('.section-body').on('click', '._open_ace_btn', function (e) {
                        mixpanel.track('click_bo_marketing_ace_apply',{'site_code':SITE_CODE});
                    });
                    break;
                // App 신청 및 관리 > Android 앱
                case '/admin/app/android':
                    // Android 앱 보기
                    mixpanel.track('view_bo_app_android',{'site_code':SITE_CODE});

                    // 신청하기 버튼 클릭
                    $('.card').on('click', '.app-btn', function (e) {
                        mixpanel.track('click_bo_app_android_apply',{'site_code':SITE_CODE});
                    });
                    break;
                // 결제/앱스토어
                case '/admin/payment':
                    if (!location.search) {
                        // 결제/앱스토어 보기
                        mixpanel.track('view_bo_appstore_payment',{'site_code':SITE_CODE});
                    } else if (params.get('mode') === 'history') {
                        // 결제내역 보기
                        mixpanel.track('view_bo_payment_history',{'site_code':SITE_CODE});
                    } else if (params.get('mode') === 'detail') {
                        // 결제 페이지 보기
                        mixpanel.track('view_bo_checkout', {
	                        'site_code':SITE_CODE,
                            'product_category': params.get('type')
                        });
                    } else if (params.get('mode') === 'service_detail') {
											const detail = params.get('detail');

											// 사용 가능한 앱 상세페이지
	                    const pageViewEvent = {
												kiwisnap: 'view_bo_appstore_kiwisnap',
		                    ifdo: 'view_bo_appstore_ifdo',
	                    }[detail];

											if (pageViewEvent)  {
												mixpanel.track(pageViewEvent, {
													site_code: SITE_CODE,
												});
											}
                    }
		                $('[data-mixpanel="click_bo_appstore_app_add"]').on('click', function (e) {
			                mixpanel.track('click_bo_appstore_app_add', {
												'app_id': $(this).data('app-id'),
				                'app_name': $(this).data('app-name'),
				                'site_code':SITE_CODE
			                });
		                });

                    // Google 개발자 계정 등록 버튼 클릭
                    $('.section-body').on('click', '.android-link', function (e) {
                        mixpanel.track('click_bo_app_android_google_developer',{'site_code':SITE_CODE});
                    });
                    break;
                // App 신청 및 관리 > iOS 앱
                case '/admin/app/ios':
                    // Android 앱 보기
                    mixpanel.track('view_bo_app_ios',{'site_code':SITE_CODE});

                    // 신청하기 버튼 클릭
                    $('.card').on('click', '.app-btn.ios', function (e) {
                        mixpanel.track('click_bo_app_ios_apply',{'site_code':SITE_CODE});
                    });
                    break;
                // 통계 > 개요
                case '/admin/stat/summary':
                    // 통계 개요 보기
                    mixpanel.track('view_bo_stat_summary',{'site_code':SITE_CODE});
                    break;
                // 통계 > 기간별 분석
                case '/admin/stat/analytics':
                    // 기간별 분석 보기
                    mixpanel.track('view_bo_stat_analytics',{'site_code':SITE_CODE});

                    // 요약 보기 클릭
                    $('.section-body').on('change', '#analyticsType', function (e) {
                        let value = $(e.currentTarget).val();
                        let event = '';

                        if (value === 'days') {
                            event = 'click_bo_stat_analytics_daily';
                        } else if (value === 'month'){
                            event = 'click_bo_stat_analytics_monthly';
                        }

                        if (event) {
                            mixpanel.track(event,{'site_code':SITE_CODE});
                        }
                    });

                    // 새로고침 클릭
                    $('.section-body').on('click', '.btn-refresh', function (e) {
                        mixpanel.track('click_bo_stat_analytics_refresh',{'site_code':SITE_CODE});
                    });

                    // 내보내기 클릭
                    $('.section-body').on('click', '.btn-download', function (e) {
                        mixpanel.track('click_bo_stat_analytics_export',{'site_code':SITE_CODE});
                    });
                    break;
                // 통계 > 적립금 통계
                case '/admin/stat/mileage':
                    // 적립금 통계 보기
                    mixpanel.track('view_bo_stat_mileage',{'site_code':SITE_CODE});

                    // 조회 클릭
                    $('.section-body').on('click', '#mileageSearchForm-submit', function (e) {
                        mixpanel.track('search_bo_stat_mileage', {
	                        'site_code':SITE_CODE,
                            'type': $('#viewMode').val(),
                            'start': $('[name="start_date"]').val(),
                            'end': $('[name="end_date"]').val()
                        });
                    });
                    break;
                // 통계 > 매출 통계
                case '/admin/stat/shopping':
                    // 매출 통계 보기
                    mixpanel.track('view_bo_stat_shopping',{'site_code':SITE_CODE});

                    $('.section-body').on('click', '.search-type a', function (e) {
                        // 조회 타입별 보기 클릭
                        mixpanel.track('click_bo_stat_shopping_view_type', {
	                        'site_code':SITE_CODE,
                            'type': $(e.currentTarget).text()
                        });
                    }).on('click', '#filterWrap', function (e) {
                        // 필터 클릭
                        mixpanel.track('click_bo_stat_shopping_filter',{'site_code':SITE_CODE});
                    });

                    // 필터 적용
                    $('#filterTypeWrap').on('click', '.btn-submit', function (e) {
                        mixpanel.track('save_bo_stat_shopping_filter', {
	                        'site_code':SITE_CODE,
                            'payment_method': $('[name="filter_pay_type[]"]').val(),
                            'device': $('[name="filter_order_channel"]:checked').val(),
                            'product': $('[name="filter_prod_code[][]"]').val(),
                            'consumer_id': $('[name="filter_orderer_id"]').val()
                        });
                    });
                    break;
                // 통계 > 컨텐츠 반응
                case '/admin/stat/feedback':
                    // 컨텐츠 반응 보기
                    mixpanel.track('view_bo_stat_feedback',{'site_code':SITE_CODE});
                    break;
                // 통계 > 방문자 통계
                case '/admin/stat/visitor':
                    // 방문자 통계 보기
                    mixpanel.track('view_bo_stat_visitor',{'site_code':SITE_CODE});

                    // 조회 타입별 보기 클릭
                    $('.section-body').on('change', '#year_month_stats', function (e) {
                        mixpanel.track('click_bo_stat_visitor_view_type', {
	                        'site_code':SITE_CODE,
                            'type': $(e.currentTarget).val()
                        });
                    });
                    break;
                // 통계 > 유입 사이트
                case '/admin/stat/site':
                    // 유입 사이트 보기
                    mixpanel.track('view_bo_stat_referer',{'site_code':SITE_CODE});

                    // 조회 타입별 보기 클릭
                    $('.section-body').on('change', '#search-type', function (e) {
                        mixpanel.track('click_bo_stat_referer_view_type', {
	                        'site_code':SITE_CODE,
                            'type': $(e.currentTarget).val()
                        });
                    });

                    // 내보내기 클릭
                    $('.card').on('click', '.btn-download-daily', function (e) {
                        mixpanel.track('click_bo_stat_referer_domain_daily_export',{'site_code':SITE_CODE});
                    }).on('click', '.btn-download', function (e) {
                        mixpanel.track('click_bo_stat_referer_domain_export',{'site_code':SITE_CODE});
                    }).on('click', '.btn-url-download-daily', function (e) {
                        mixpanel.track('click_bo_stat_referer_url_daily_export',{'site_code':SITE_CODE});
                    }).on('click', '.btn-url-download', function (e) {
                        mixpanel.track('click_bo_stat_referer_url_export',{'site_code':SITE_CODE});
                    });
                    break;
                // 통계 > 유입 검색어
                case '/admin/stat/keyword':
                    // 유입 검색어 보기
                    mixpanel.track('view_bo_stat_keyword',{'site_code':SITE_CODE});

                    // 조회 타입별 보기 클릭
                    $('.section-body').on('change', '#search-type', function (e) {
                        mixpanel.track('click_bo_stat_referer_view_type', {
	                        'site_code':SITE_CODE,
                            'type': $(e.currentTarget).val()
                        });
                    });
                    break;
                // 통계 > 많이 방문한 페이지
                case '/admin/stat/page':
                    // 많이 방문한 페이지 보기
                    mixpanel.track('view_bo_stat_page',{'site_code':SITE_CODE});

                    // 조회 타입별 보기 클릭
                    $('.section-body').on('change', '#search-type', function (e) {
                        mixpanel.track('click_bo_stat_page_view_type', {
	                        'site_code':SITE_CODE,
                            'type': $(e.currentTarget).val()
                        });
                    });
                    break;
                // 통계 > 저장공간
                case '/admin/stat/storage':
                    // 저장공간 보기
                    mixpanel.track('view_bo_stat_storage',{'site_code':SITE_CODE});
                    break;
                // 통계 > 트래픽
                case '/admin/stat/traffic':
                    // 트래픽 보기
                    mixpanel.track('view_bo_stat_traffic',{'site_code':SITE_CODE});
                    break;
                // 통계 > 부가 서비스
                case '/admin/stat/additional_service':
                    // 트래픽 보기
                    mixpanel.track('view_bo_stat_external',{'site_code':SITE_CODE});
                    break;
                // 통계 > 라플라스 애널리틱스
                case '/admin/stat/laplace_analytics':
                    // 라플라스 애널리틱스 보기
                    mixpanel.track('view_bo_stat_laplace',{'site_code':SITE_CODE});

                    // 서비스 신청버튼 클릭
                    $('.top_header').on('click', '.btn-laplace', function (e) {
                        mixpanel.track('click_bo_stat_laplace_apply',{'site_code':SITE_CODE});
                    });

                    // 서비스 신청버튼 클릭
                    $('#stat_laplace').on('click', 'a', function (e) {
                        mixpanel.track('click_bo_stat_laplace_apply',{'site_code':SITE_CODE});
                    });
                    break;
				// 통계 > 에이스 카운터
	            case '/admin/stat/ace_counter':
					//에이스 카운터 보기
		            mixpanel.track('view_bo_stat_ace_counter',{'site_code':SITE_CODE});

					// 서비스 신청 버튼 클릭
		            $('[data-mixpanel="click_bo_stat_ace_counter_apply"]').on('click', function (e) {
			            mixpanel.track('click_bo_stat_ace_counter_apply', {
				            'site_code':SITE_CODE
			            });
		            });
		            break;
	            // 디자인 모드
                case '/admin/design':
                    // 디자인모드 보기
                    mixpanel.track('view_dm',{'site_code':SITE_CODE});

                    // 버튼 클릭
                    $('body').on('click', '#menu_list_btn', function () {
                        // 메뉴 관리 클릭(좌측 상단)
                        mixpanel.track('click_dm_menu_left_top',{'site_code':SITE_CODE});
                    }).on('click', '._publish', function () {
                        // 게시하기 클릭
                        mixpanel.track('click_dm_publish',{'site_code':SITE_CODE});
                    }).on('click', '._leave', function () {
                        // 나가기 클릭
                        mixpanel.track('click_dm_leave',{'site_code':SITE_CODE});
                    }).on('click', '#reset_design_temp', function () {
                        // 복구하기 클릭
                        mixpanel.track('click_dm_undo',{'site_code':SITE_CODE});
                    }).on('click', '._preview', function () {
                        // 미리보기 클릭
                        mixpanel.track('click_dm_preview',{'site_code':SITE_CODE});
                    }).on('click', '#design_setting_btn2', function () {
                        // 공통 디자인 설정 클릭
                        mixpanel.track('click_dm_settings',{'site_code':SITE_CODE});
                    }).on('click', '._empty_section span', function () {
                        // 위젯 추가 클릭
                        mixpanel.track('click_dm_add_widget', {
	                        'site_code':SITE_CODE,
                            'source': 'body'
                        });
                    }).on('click', '._widget_add', function () {
                        // 위젯 추가 클릭
                        mixpanel.track('click_dm_add_widget', {
	                        'site_code':SITE_CODE,
                            'source': 'left_nav'
                        });
                    }).on('click', '._section_add', function () {
                        // 섹션 추가 클릭
                        mixpanel.track('click_dm_add_section', {
	                        'site_code':SITE_CODE,
                            'source': 'left_nav'
                        });
                    }).on('click', '._empty._item', function () {
                        // 섹션 추가 클릭
                        mixpanel.track('click_dm_add_section', {
	                        'site_code':SITE_CODE,
                            'source': 'body'
                        });
                    }).on('click', '._menu_info', function () {
                        // 메뉴 설정 클릭
                        mixpanel.track('click_dm_menu_settings', {
	                        'site_code':SITE_CODE,
                            'source': 'top'
                        });
                    }).on('click', '#design_help_on', function () {
                        // 도움말 클릭 (우측 하단));
                        mixpanel.track('click_dm_help',{'site_code':SITE_CODE});
                    }).on('click', '#main_menu_add, #foot_menu_add', function () {
                        // 메뉴 추가 버튼 클릭 (메뉴설정 내)
                        mixpanel.track('click_dm_add_page',{'site_code':SITE_CODE});
                    });

                    // 메뉴설정 내 글로벌 메뉴 클릭
                    $('._global_menu_container ._sel').on('click', function (e) {
                        mixpanel.track('click_dm_global_menu', {
	                        'site_code':SITE_CODE,
                            'type': $(e.currentTarget).data('url')
                        });
                    });

                    // 디바이스 아이콘 클릭
                    $('#device_type').on('click', function () {
                        mixpanel.track('click_dm_device_button',{'site_code':SITE_CODE});
                    });
                    break;
								// 환경설정 > 소셜 로그인 · 지도 · 공유
	              case '/admin/config/oauth':
										// 소셜 로그인 · 지도 · 공유 조회
										mixpanel.track('view_bo_social_map_share', {
												site_code: SITE_CODE,
										});
										break;
                default:
                    break;
            }
        };
    }
};

/**
 * benefit_type에 따른 offer_type 텍스트 반환
 * @param {string} benefit_type - 혜택 유형 (discount, delivery, fix_price)
 * @returns {string} offer_type 텍스트
 */
function getOfferTypeFromBenefitType(benefit_type) {
    switch(benefit_type) {
        case 'delivery':
            return 'free shipping';
        case 'fix_price':
            return 'fixed price purchase';
        case 'discount':
        default:
            return 'discount purchase price';
    }
}
                case '/admin/member/send':
									const mode = params.get('mode');
									const type = params.get('type');
                  // 메세지 보내기 페이지 확인
                  if(location.search==='' || mode==='alarm'||mode ==='sms'){mixpanel.track('view_bo_member_message_view',{'site_code':SITE_CODE});}

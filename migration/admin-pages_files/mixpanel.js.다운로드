                    })
                    break;
		            // 사용자관리 > 사용자목록
	              case '/admin/member/list':
			            let memberViewMode = params.get('mode');
		              let memberDetailView = params.get('idx');
		              if(memberViewMode!=='add') {mixpanel.track('view_bo_member_list',{'site_code':SITE_CODE})}

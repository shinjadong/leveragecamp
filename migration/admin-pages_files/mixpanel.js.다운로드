                      }else if(e.target.id==='mixpanel_bo_member_message_notification_sent_item_delete'){
                        // 최근 예약 발송 알림 항목 예약 취소 및 삭제 클릭
                        mixpanel.track('click_bo_member_message_notification_sent_item_delete',{'site_code':SITE_CODE})
                      }
                    })
                    // 최근 예약발송 알림 페이지
                  }else if(mode==='alarm_list' && type==='booking'){
                    mixpanel.track('view_bo_member_message_notification_scheduled',{'site_code':SITE_CODE});
                    // 삭제버튼인지 항목 클릭하는 건지 확인
                    $('#mixpanel_message_notification_list').on('click',function(e){
                      if(e.target.id==='mixpanel_bo_member_message_notification_sent_item'){
                        // 최근 예약 발송 알림 항목 클릭
                        mixpanel.track('click_bo_member_message_notification_sent_item',{'site_code':SITE_CODE})
                      }else if(e.target.id==='mixpanel_bo_member_message_notification_sent_item_delete'){
                        // 최근 예약 발송 알림 항목 예약 취소 및 삭제 클릭
                        mixpanel.track('click_bo_member_message_notification_sent_item_delete',{'site_code':SITE_CODE})
                      }
                    })
                  }
                  // 최근 30일 발송 알림 클릭
                  $('#mixpanel_member_message_notification_sent').on('click',function(e){
                    mixpanel.track('click_bo_member_message_notification_sent',{'site_code':SITE_CODE});
                  })
                  // 최근 예약 발송 알림 클릭
                  $('#mixpanel_member_message_notification_scheduled').on('click',function(e){
                    mixpanel.track('click_bo_member_message_notification_scheduled',{'site_code':SITE_CODE});
                  })
                  // 메시지 보내기 상단 탭 클릭
                  $('ul.nav.nav-tabs').on('click','a',function(e){
                    let hrefValue = $(this).attr('href')
                    let nameValue = hrefValue.split('?')[1].split('=')[1];
                    mixpanel.track('click_bo_member_message_tab',{'name':nameValue,'site_code':SITE_CODE});

                  })
                  // SMS 광고성 문자메시지 전송시 주의사항 클릭
                  $('.alert.alert-info a').on('click',function(e){
                    mixpanel.track('click_bo_member_message_sms_warning',{'site_code':SITE_CODE})
                  })
                  // SMS 추가 구입 버튼 클릭
                  $('.form-group a').on('click',function(e){
                    // onclick 값으로 확인합니다.
                    if($(this).attr('onclick')==='adminMember.openChargeSmsform(\'member\')'){mixpanel.track('click_bo_member_message_sms_purchase',{'site_code':SITE_CODE});}
                  })
                  // 발신번호 등록/변경 버튼 클릭
                  $('#mixpanel_member_message_sms_add_calling').on('click',function(e){
                    mixpanel.track('click_bo_member_message_sms_add_calling',{'site_code':SITE_CODE});
                  })
                  // 문자메세지(SMS) 충전 충전하기 클릭 폼 데이터 제출 이벤트 핸들링
                  $(document).on('submit','#payment_submit_form',function(e){
                    // 부가가치세 비율 10%
                    const TAX_RATE = 0.1
                    const formData = $(this).serializeArray()
                    const priceObj = formData.find(item => item.name ==='price');
                    const selPaymentTypeObj = formData.find(item => item.name ==='sel_payment_type');
                    const priceValue = priceObj ? parseInt(priceObj.value) : null;
                    const selPaymentTypeValue = selPaymentTypeObj ? selPaymentTypeObj.value : null;
                    const priceValueTax = priceValue + (priceValue * TAX_RATE)
                    mixpanel.track('click_bo_member_message_sms_checkout',{'price':priceValueTax,'payment_method':selPaymentTypeValue,'site_code':SITE_CODE});
                  })
                  // SMS,alarm 탭 공용 보내기 버튼
                  $('#sendsms').on('click',function(e){
                    let is_sms = $(this).attr('onclick');
                    let scheduledCheckBoxValue = $('input[name="is_booking"]').is(':checked');
                    // SMS 보내기 버튼
                    if(is_sms==="send_sms()"){

                      // alarm 보내기 버튼
                    }else if(is_sms==="send_alarm()"){
                      let displayValue = $('#alarm_img_group').css('display');
                      //display 값으로 image를 보여주고 제거해주는 것을 이용합니다.
                      displayValue === 'inline-block' ? mixpanel.track('complete_bo_member_message_notification_send',{'image':true,'scheduled':scheduledCheckBoxValue,'site_code':SITE_CODE}) : mixpanel.track('complete_bo_member_message_notification_send',{'image':false,'scheduled':scheduledCheckBoxValue,'site_code':SITE_CODE});
                    }
                  })



                  break;

                // 환경설정 > 기본설정
                case '/admin/config/default':
                    mixpanel.track('view_bo_settings_general',{'site_code':SITE_CODE});
                    break;
		            case '/admin/config/membership':
			            const membershipUrlMode = params.get('mode') || "";
			            if (membershipUrlMode === "join" || membershipUrlMode === "") {
				            mixpanel.track('view_bo_settings_member_signup',{'site_code':SITE_CODE});
									}
			            if (membershipUrlMode === "group") {
				            mixpanel.track('view_bo_settings_member_group',{'site_code':SITE_CODE});
			            }
			            $(document).on('click', '[data-mixpanel="click_bo_settings_member_shopping_group_change_apply"]', function() {

				            mixpanel.track('click_bo_settings_member_shopping_group_change_apply', {'site_code': SITE_CODE});

			            });

			            console.log("mode 값", membershipUrlMode);
		              break;

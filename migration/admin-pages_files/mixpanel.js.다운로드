                    $('#cocoaSubModal').on('click', '#mixpanel_domain_enroll_payment', function(e){
                      let totalPriceText = $('.text-primary._total_price').text();
                      let totalPriceNumber = parseInt(totalPriceText.replace(',', '').replace('원', ''));
                      let paymentMethodValue = $('input[name=sel_payment_type]:checked').val();
                      let selectedValue = parseInt($('input[name=period]:checked').val());
                      let domainNameValue = $('#formModalLabel').text().split(' ')[0];
                      mixpanel.track('click_bo_domain_buy_domain_detail',{'name':domainNameValue,'count':selectedValue,'price':totalPriceNumber,'payment_method':paymentMethodValue,'site_code':SITE_CODE});
                    })

                    break;
